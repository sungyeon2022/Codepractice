package first_Java_project;

import java.util.*;

public class Solution {
	public int[] solution(int []arr) {
		int[] answer = {};
		int anln = 1;
		answer[0]=arr[0];
		for(int i = 1;i<arr.length;i++) {
			if(arr[i]!=arr[i-1]) {
				answer[anln]=arr[i];
				anln++;
			}
		}
		return answer;
	}
	public static void main(String[] args) {
	}
}
