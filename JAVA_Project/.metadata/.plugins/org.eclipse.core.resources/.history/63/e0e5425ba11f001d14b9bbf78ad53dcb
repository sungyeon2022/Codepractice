package first_Java_project;

import java.util.*;

public class Solution {
	public String solution(String new_id) {
		String answer = "";
		for(int i =0;i<new_id.length();i++) {
			if(new_id.charAt(i)>=65&&new_id.charAt(i)<=90) {
				new_id.charAt(i)+='32';
			}
		}
        return answer;
	}
	public static void main(String[] args) {
	}
}
